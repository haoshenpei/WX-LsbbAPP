}
.lvs-noData  {
  width: 430rpx;
  height: 450rpx;
  text-align: center;
}
.lvs-noData image{
  margin: 0 auto;
}
.lvs-noData .test{
  height: 72rpx;
  width: 327rpx;
  border: solid 1px #577eff;
  border-radius: 10rpx;
  color: #577eff;
  font-size: 32rpx;
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: center;
}
.lvs-noData .margin {
